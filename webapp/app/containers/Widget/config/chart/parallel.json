{
  "id": 21,
  "name": "parallel",
  "title": "平行坐标轴",
  "renderer": "echarts",
  "params": [{
    "name": "data",
    "title": "数据项配置",
    "items": [{
      "name": "metrics",
      "title": "指标",
      "component": "multiSelect"
    }, {
      "name": "categories",
      "title": "分类",
      "component": "multiSelect"
    }, {
      "name": "groups",
      "title": "分组",
      "component": "multiSelect"
    }]
  }, {
    "name": "tools",
    "title": "工具",
    "items": [{
      "name": "tooltip",
      "title": "Tooltip",
      "component": "checkbox"
    }, {
      "name": "toolbox",
      "title": "Toolbox",
      "component": "checkbox"
    }]
  }, {
    "name": "legend",
    "title": "图例",
    "items": [{
      "name": "hasLegend",
      "title": "开启图例",
      "component": "checkbox",
      "span": 24
    }, {
      "name": "legendSelected",
      "component": "radio",
      "values": [{
        "name": "默认全选",
        "value": "selectAll"
      }, {
        "name": "默认全不选",
        "value": "unselectAll"
      }],
      "default": "selectAll",
      "span": 24
    }, {
      "name": "legendPosition",
      "title": "图例位置",
      "component": "radio",
      "values": [{
        "name": "顶部",
        "value": "top"
      }, {
        "name": "底部",
        "value": "bottom"
      }, {
        "name": "右侧",
        "value": "right"
      }],
      "default": "top",
      "span": 24
    }]
  }, {
    "name": "position",
    "title": "边距比例",
    "items": [{
        "name": "top",
        "title": "上边距",
        "component": "inputnumber",
        "min": 0,
        "max": 100,
        "default": 10
      },
      {
        "name": "bottom",
        "title": "下边距",
        "component": "inputnumber",
        "min": 0,
        "max": 100,
        "default": 10
      },
      {
        "name": "left",
        "title": "左边距",
        "component": "inputnumber",
        "min": 0,
        "max": 100,
        "default": 10
      },
      {
        "name": "right",
        "title": "右边距",
        "component": "inputnumber",
        "min": 0,
        "max": 100,
        "default": 10
      }
    ]
  }]
}
