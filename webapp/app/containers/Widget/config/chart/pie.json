{
  "id": 4,
  "name": "pie",
  "title": "饼图",
  "renderer": "echarts",
  "params": [
    {
      "name": "data",
      "title": "数据项配置",
      "items": [
        {
          "name": "title",
          "title": "标题",
          "component": "select"
        },
        {
          "name": "value",
          "title": "数值",
          "component": "select"
        }
      ]
    },
    {
      "name": "display",
      "title": "显示",
      "items": [
        {
          "name": "roseType",
          "title": "南丁格尔玫瑰",
          "component": "checkbox",
          "span": 12
        },
        {
          "name": "circle",
          "title": "环状",
          "component": "checkbox",
          "span": 12
        },
        {
          "name": "insideRadius",
          "title": "直径比例",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 60
        },
        {
          "name": "outsideRadius",
          "title": "外环直径比例",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 70
        }
      ]
    },
    {
      "name": "legend",
      "title": "图例",
      "items": [
        {
          "name": "hasLegend",
          "title": "开启图例",
          "component": "checkbox",
          "span": 24
        },
        {
          "name": "legendSelected",
          "component": "radio",
          "values": [{
            "name": "默认全选",
            "value": "selectAll"
          }, {
            "name": "默认全不选",
            "value": "unselectAll"
          }],
          "default": "selectAll",
          "span": 24
        },
        {
          "name": "legendPosition",
          "title": "图例位置",
          "component": "radio",
          "values": [{
            "name": "顶部",
            "value": "top"
          }, {
            "name": "底部",
            "value": "bottom"
          }, {
            "name": "右侧",
            "value": "right"
          }],
          "default": "top",
          "span": 24
        }
      ]
    },
    {
      "name": "tools",
      "title": "工具",
      "items": [
        {
          "name": "toolbox",
          "title": "Toolbox",
          "component": "checkbox"
        }
      ]
    },
    {
      "name": "position",
      "title": "边距比例",
      "items": [
        {
          "name": "top",
          "title": "上边距",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 50
        },
        {
          "name": "left",
          "title": "左边距",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 50
        }
      ]
    }
  ]
}
