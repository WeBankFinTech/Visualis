{
  "id": 4,
  "name": "pie",
  "title": "饼图",
  "renderer": "echarts",
  "params": [
    {
      "name": "data",
      "title": "数据项配置",
      "items": [
        {
          "name": "title",
          "title": "标题",
          "component": "select"
        },
        {
          "name": "value",
          "title": "数值",
          "component": "select"
        }
      ]
    },
    {
      "name": "display",
      "title": "显示",
      "items": [
        {
          "name": "circle",
          "title": "环状",
          "component": "checkbox",
          "span": 24
        },
        {
          "name": "insideRadius",
          "title": "直径比例",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 60
        },
        {
          "name": "outsideRadius",
          "title": "外环直径比例",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 70
        }
      ]
    },
    {
      "name": "tools",
      "title": "工具",
      "items": [
        {
          "name": "tooltip",
          "title": "Tooltip",
          "component": "checkbox"
        },
        {
          "name": "legend",
          "title": "Legend",
          "component": "checkbox"
        },
        {
          "name": "toolbox",
          "title": "Toolbox",
          "component": "checkbox"
        }
      ]
    },
    {
      "name": "position",
      "title": "边距比例",
      "items": [
        {
          "name": "top",
          "title": "Top",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 50
        },
        {
          "name": "left",
          "title": "Left",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 50
        }
      ]
    }
  ]
}
