{
  "id": 7,
  "name": "funnel",
  "title": "漏斗图",
  "renderer": "echarts",
  "params": [
    {
      "name": "data",
      "title": "数据项配置",
      "items": [
        {
          "name": "title",
          "title": "标题",
          "component": "select"
        },
        {
          "name": "value",
          "title": "数值",
          "component": "select"
        },
        {
          "name": "min",
          "title": "最小值",
          "component": "inputnumber"
        },
        {
          "name": "max",
          "title": "最大值",
          "component": "inputnumber"
        }
      ]
    },
    {
      "name": "display",
      "title": "显示",
      "items": [
        {
          "name": "gap",
          "title": "间距",
          "component": "inputnumber"
        }
      ]
    },
    {
      "name": "legend",
      "title": "图例",
      "items": [
        {
          "name": "hasLegend",
          "title": "开启图例",
          "component": "checkbox",
          "span": 24
        },
        {
          "name": "legendSelected",
          "component": "radio",
          "values": [{
            "name": "默认全选",
            "value": "selectAll"
          }, {
            "name": "默认全不选",
            "value": "unselectAll"
          }],
          "default": "selectAll",
          "span": 24
        },
        {
          "name": "legendPosition",
          "title": "图例位置",
          "component": "radio",
          "values": [{
            "name": "顶部",
            "value": "top"
          }, {
            "name": "底部",
            "value": "bottom"
          }, {
            "name": "右侧",
            "value": "right"
          }],
          "default": "top",
          "span": 24
        }
      ]
    },
    {
      "name": "tools",
      "title": "工具",
      "items": [
        {
          "name": "toolbox",
          "title": "Toolbox",
          "component": "checkbox"
        }
      ]
    },
    {
      "name": "size",
      "title": "图形比例",
      "items": [
        {
          "name": "width",
          "title": "宽",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 80
        },
        {
          "name": "height",
          "title": "高",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 80
        },
        {
          "name": "minSize",
          "title": "最小值映射比例",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 0
        },
        {
          "name": "maxSize",
          "title": "最大值映射比例",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 100
        }
      ]
    },
    {
      "name": "position",
      "title": "边距比例",
      "items": [
        {
          "name": "top",
          "title": "上边距",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 10
        },
        {
          "name": "left",
          "title": "左边距",
          "component": "inputnumber",
          "min": 0,
          "max": 100,
          "default": 10
        }
      ]
    }
  ]
}
