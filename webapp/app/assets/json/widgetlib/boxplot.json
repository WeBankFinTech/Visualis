{
  "id": 14,
  "name": "boxplot",
  "title": "箱线图",
  "renderer": "echarts",
  "params": [
    {
      "name": "data",
      "title": "数据项配置",
      "items": [
        {
          "name": "xAxis",
          "title": "X轴",
          "component": "select"
        },
        {
          "name": "metrics",
          "title": "显示指标",
          "component": "multiSelect"
        },
        {
          "name": "suffixYAxis",
          "title": "Y轴后缀",
          "component": "input"
        }
      ]
    },
    {
      "name": "display",
      "title": "显示",
      "items": [
        {
          "name": "dataZoomThreshold",
          "title": "超出后缩放",
          "component": "inputnumber"
        },
        {
          "name": "xAxisRotate",
          "title": "x轴文字旋转角度",
          "component": "inputnumber"
        },
        {
          "name": "xAxisInterval",
          "title": "x轴刻度显示间隔",
          "component": "inputnumber"
        }
      ]
    },
    {
      "name": "legend",
      "title": "图例",
      "items": [
        {
          "name": "hasLegend",
          "title": "开启图例",
          "component": "checkbox",
          "span": 24
        },
        {
          "name": "legendSelected",
          "component": "radio",
          "values": [{
            "name": "默认全选",
            "value": "selectAll"
          }, {
            "name": "默认全不选",
            "value": "unselectAll"
          }],
          "default": "selectAll",
          "span": 24
        },
        {
          "name": "legendPosition",
          "title": "图例位置",
          "component": "radio",
          "values": [{
            "name": "顶部",
            "value": "top"
          }, {
            "name": "底部",
            "value": "bottom"
          }, {
            "name": "右侧",
            "value": "right"
          }],
          "default": "top",
          "span": 24
        }
      ]
    },
    {
      "name": "tools",
      "title": "工具",
      "items": [
        {
          "name": "toolbox",
          "title": "Toolbox",
          "component": "checkbox"
        }
      ]
    },
    {
      "name": "splitLine",
      "title": "分隔线",
      "items": [
        {
          "name": "splitLineX",
          "title": "x轴分隔线",
          "component": "checkbox"
        },
        {
          "name": "splitLineY",
          "title": "y轴分隔线",
          "component": "checkbox"
        },
        {
          "name": "splitLineStyle",
          "title": "样式",
          "component": "select",
          "values": [
            {
              "name": "直线",
              "value": "solid"
            },
            {
              "name": "虚线",
              "value": "dashed"
            },
            {
              "name": "点线",
              "value": "dotted"
            }
          ],
          "default": "solid",
          "span": 12
        },
        {
          "name": "splitLineWidth",
          "title": "宽度",
          "component": "select",
          "values": [
            {
              "name": "1",
              "value": "1"
            },
            {
              "name": "2",
              "value": "2"
            },
            {
              "name": "3",
              "value": "3"
            },
            {
              "name": "5",
              "value": "5"
            },
            {
              "name": "10",
              "value": "10"
            }
          ],
          "default": "1",
          "span": 12
        }
      ]
    },
    {
      "name": "position",
      "title": "边距",
      "items": [
        {
          "name": "top",
          "title": "上边距",
          "component": "inputnumber",
          "default": 45
        },
        {
          "name": "bottom",
          "title": "下边距",
          "component": "inputnumber",
          "default": 40
        },
        {
          "name": "left",
          "title": "左边距",
          "component": "inputnumber",
          "default": 50
        },
        {
          "name": "right",
          "title": "右边距",
          "component": "inputnumber",
          "default": 30
        }
      ]
    }
  ]
}
