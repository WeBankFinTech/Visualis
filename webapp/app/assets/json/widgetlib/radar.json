{
  "id": 19,
  "name": "radar",
  "title": "雷达图",
  "renderer": "echarts",
  "params": [
    {
      "name": "data",
      "title": "数据项配置",
      "items": [
        {
          "name": "indicator",
          "title": "顶点",
          "component": "multiSelect"
        },
        {
          "name": "metrics",
          "title": "显示指标",
          "component": "select"
        },
        {
          "name": "metricName",
          "title": "指标名称",
          "component": "input"
        }
      ]
    },
    {
      "name": "legend",
      "title": "图例",
      "items": [
        {
          "name": "hasLegend",
          "title": "开启图例",
          "component": "checkbox",
          "span": 24
        },
        {
          "name": "legendSelected",
          "component": "radio",
          "values": [{
            "name": "默认全选",
            "value": "selectAll"
          }, {
            "name": "默认全不选",
            "value": "unselectAll"
          }],
          "default": "selectAll",
          "span": 24
        },
        {
          "name": "legendPosition",
          "title": "图例位置",
          "component": "radio",
          "values": [{
            "name": "顶部",
            "value": "top"
          }, {
            "name": "底部",
            "value": "bottom"
          }, {
            "name": "右侧",
            "value": "right"
          }],
          "default": "top",
          "span": 24
        }
      ]
    },
    {
      "name": "tools",
      "title": "工具",
      "items": [
        {
          "name": "toolbox",
          "title": "Toolbox",
          "component": "checkbox"
        }
      ]
    },
    {
      "name": "position",
      "title": "边距",
      "items": [
        {
          "name": "top",
          "title": "上边距",
          "component": "inputnumber",
          "default": 45
        },
        {
          "name": "bottom",
          "title": "下边距",
          "component": "inputnumber",
          "default": 40
        },
        {
          "name": "left",
          "title": "左边距",
          "component": "inputnumber",
          "default": 50
        },
        {
          "name": "right",
          "title": "右边距",
          "component": "inputnumber",
          "default": 30
        }
      ]
    }
  ]
}
